<div class="bg-white row main-container p-4">
    <div class="d-flex justify-content-between">
        <div class="list_title">Send Email List</div>                   
    </div>
    <div class="table-responsive mt-3" >
        <table class="table custombordercolor" id="sendEmail">
            <thead>
                <tr>
                    <th scope="col">Sl NO</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>                   
                    <th scope="col">Mobile</th>
                    <th scope="col">Scheduled Date</th>    
                </tr>
            </thead>
            <tbody>
                <% for (var i=0; i < data.length; i++) { %>
                    <tr>
                        <td>
                            <%= i+1; %>
                        </td>
                        <td>
                            <%= data[i].name; %>
                        </td>
                        <td>
                            <%= data[i].email; %>
                        </td>                        
                        <td>
                            <%= data[i].mobile; %>
                        </td>
                        <td>
                            <%= data[i].scheduled_date; %>
                        </td>                        
                        </tr>
                    <% } %>
            </tbody>
        </table>
    </div>
</div>
<script>
    $(document).ready(function(){       
        $('#sendEmail').DataTable({
            paging: true,       
            searching: false,    
            ordering: false,   
            lengthChange: false  
        });
    });
</script>